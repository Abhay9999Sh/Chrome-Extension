{
    "manifest_version": 3,
    "name": "LinkedIn Profile Scraper",
    "version": "1.0",
    "permissions": [
      "tabs", 
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://localhost:3000/*" 
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/tab-icon.jpg",
        "48": "images/tab-icon.jpg",
        "128": "images/tab-icon.jpg"
      }
    }
  }
  